&НаСервере
Функция  ПолучитьИнтервалДиаграммы(Проект, тзДатыОкончания) Экспорт

	стИнтервал = Новый Структура("ДатаНачала,ДатаОкончания",Дата(1,1,1), Дата(1,1,1));
	
	тзОкончанияСделка = Новый ТаблицаЗначений;
	тзОкончанияСделка.Колонки.Добавить("Проект", Новый ОписаниеТипов("СправочникСсылка.Проекты"));
	тзОкончанияСделка.Колонки.Добавить("ОкончаниеСЗапасом", Новый ОписаниеТипов("Дата"));
	тзОкончанияСделка.Индексы.Добавить("Проект");
	
	Для каждого х Из тзДатыОкончания Цикл
		
		НайденаяСтрока = тзОкончанияСделка.Найти(х.Проект,"Проект");
		Если НайденаяСтрока = Неопределено Тогда
			ЗаполнитьЗначенияСвойств(тзОкончанияСделка.Добавить() ,х);	
		Иначе	
		   	Если НайденаяСтрока.ОкончаниеСЗапасом < х.ОкончаниеМонтаж Тогда
				НайденаяСтрока.ОкончаниеСЗапасом = х.ОкончаниеМонтаж;
			КонецЕсли;
			
		   	Если НайденаяСтрока.ОкончаниеСЗапасом < х.ОкончаниеСЗапасом Тогда
				НайденаяСтрока.ОкончаниеСЗапасом = х.ОкончаниеСЗапасом;
			КонецЕсли;
		КонецЕсли;
	
	КонецЦикла;

	Если ЗначениеЗаполнено(Проект) Тогда
		Если ТипЗнч(Проект) = Тип("СправочникСсылка.Проекты") Тогда
		
			Возврат ПолучитьИнтервалДиаграммыОднаСделка(Проект,стИнтервал, тзОкончанияСделка);	
		
		Иначе
		
			Возврат ПолучитьИнтервалДиаграммыМассивСделок(Проект, тзОкончанияСделка);	
		
		КонецЕсли;
	
	Иначе
	
		Возврат Новый ТаблицаЗначений;	
	
	КонецЕсли;
КонецФункции //  ПолучитьИнтервалДиаграммы(Сделка)

&НаСервере
Функция ПолучитьИнтервалДиаграммыМассивСделок(мСделок, тзДатыОкончания)
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	ДатыКомплекта.Проект КАК Проект,
	|	ЕСТЬNULL(МАКСИМУМ(ДатыКомплекта.КрайПКД), ДАТАВРЕМЯ(1, 1, 1)) КАК Дата
	|ИЗ
	|	РегистрСведений.ДатыКомплекта КАК ДатыКомплекта
	|ГДЕ
	|	ДатыКомплекта.Проект В(&мСделок)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДатыКомплекта.Проект
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ДатыКомплекта.Проект КАК Проект,
	|	ЕСТЬNULL(МИНИМУМ(ДатыКомплекта.НачалоПКД), ДАТАВРЕМЯ(1, 1, 1)) КАК Дата
	|ИЗ
	|	РегистрСведений.ДатыКомплекта КАК ДатыКомплекта
	|ГДЕ
	|	ДатыКомплекта.Проект В(&мСделок)
	|	И ДатыКомплекта.НачалоПКД > ДАТАВРЕМЯ(1, 1, 1)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДатыКомплекта.Проект
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ДатыКомплекта.Проект КАК Проект,
	|	ЕСТЬNULL(МАКСИМУМ(ДатыКомплекта.КрайРКД), ДАТАВРЕМЯ(1, 1, 1)) КАК Дата
	|ИЗ
	|	РегистрСведений.ДатыКомплекта КАК ДатыКомплекта
	|ГДЕ
	|	ДатыКомплекта.Проект В(&мСделок)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДатыКомплекта.Проект
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ДатыКомплекта.Проект КАК Проект,
	|	ЕСТЬNULL(МИНИМУМ(ДатыКомплекта.НачалоРКД), ДАТАВРЕМЯ(1, 1, 1)) КАК Дата
	|ИЗ
	|	РегистрСведений.ДатыКомплекта КАК ДатыКомплекта
	|ГДЕ
	|	ДатыКомплекта.Проект В(&мСделок)
	|	И ДатыКомплекта.НачалоРКД > ДАТАВРЕМЯ(1, 1, 1)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДатыКомплекта.Проект
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ДатыКомплекта.Проект КАК Проект,
	|	ЕСТЬNULL(МАКСИМУМ(ДатыКомплекта.КрайТЗ), ДАТАВРЕМЯ(1, 1, 1)) КАК Дата
	|ИЗ
	|	РегистрСведений.ДатыКомплекта КАК ДатыКомплекта
	|ГДЕ
	|	ДатыКомплекта.Проект В(&мСделок)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДатыКомплекта.Проект
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ДатыКомплекта.Проект КАК Проект,
	|	ЕСТЬNULL(МИНИМУМ(ДатыКомплекта.НачалоТЗ), ДАТАВРЕМЯ(1, 1, 1)) КАК Дата
	|ИЗ
	|	РегистрСведений.ДатыКомплекта КАК ДатыКомплекта
	|ГДЕ
	|	ДатыКомплекта.Проект В(&мСделок)
	|	И ДатыКомплекта.НачалоТЗ > ДАТАВРЕМЯ(1, 1, 1)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДатыКомплекта.Проект
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ДатыКомплектаМонтаж.Проект КАК Проект,
	|	ЕСТЬNULL(МАКСИМУМ(ДатыКомплектаМонтаж.КрайМонтажа), ДАТАВРЕМЯ(1, 1, 1)) КАК Дата
	|ИЗ
	|	РегистрСведений.ДатыКомплектаМонтаж КАК ДатыКомплектаМонтаж
	|ГДЕ
	|	ДатыКомплектаМонтаж.Проект В(&мСделок)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДатыКомплектаМонтаж.Проект
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ДатыКомплектаМонтаж.Проект КАК Проект,
	|	ЕСТЬNULL(МИНИМУМ(ДатыКомплектаМонтаж.НачалоМонтажа), ДАТАВРЕМЯ(1, 1, 1)) КАК Дата
	|ИЗ
	|	РегистрСведений.ДатыКомплектаМонтаж КАК ДатыКомплектаМонтаж
	|ГДЕ
	|	ДатыКомплектаМонтаж.Проект В(&мСделок)
	|	И ДатыКомплектаМонтаж.НачалоМонтажа > ДАТАВРЕМЯ(1, 1, 1)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДатыКомплектаМонтаж.Проект
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ДатыКомплектаПроизводство.Проект КАК Проект,
	|	ЕСТЬNULL(МАКСИМУМ(ДатыКомплектаПроизводство.КрайПроизводства), ДАТАВРЕМЯ(1, 1, 1)) КАК Дата
	|ИЗ
	|	РегистрСведений.ДатыКомплектаПроизводство КАК ДатыКомплектаПроизводство
	|ГДЕ
	|	ДатыКомплектаПроизводство.Проект В(&мСделок)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДатыКомплектаПроизводство.Проект
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	ДатыКомплектаПроизводство.Проект КАК Проект,
	|	ЕСТЬNULL(МИНИМУМ(ДатыКомплектаПроизводство.НачалоПроизводства), ДАТАВРЕМЯ(1, 1, 1)) КАК Дата
	|ИЗ
	|	РегистрСведений.ДатыКомплектаПроизводство КАК ДатыКомплектаПроизводство
	|ГДЕ
	|	ДатыКомплектаПроизводство.Проект В(&мСделок)
	|	И ДатыКомплектаПроизводство.НачалоПроизводства > ДАТАВРЕМЯ(1, 1, 1)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДатыКомплектаПроизводство.Проект";
	
	Запрос.УстановитьПараметр("мСделок", мСделок);
	ВыборкаПакет = Запрос.ВыполнитьПакет(); 
	
	ТипДата = Новый ОписаниеТипов("Дата");
	тзИнтервалыСделок = Новый ТаблицаЗначений;
	тзИнтервалыСделок.Колонки.Добавить("Проект", 		Новый ОписаниеТипов("СправочникСсылка.Проекты"));
	тзИнтервалыСделок.Колонки.Добавить("ДатаНачала", 	ТипДата);
	тзИнтервалыСделок.Колонки.Добавить("ДатаОкончания", ТипДата);
	тзИнтервалыСделок.Индексы.Добавить("Проект");
	
	Выборка = ВыборкаПакет[0].Выбрать();
	ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Ложь, Выборка);
	
	Выборка = ВыборкаПакет[2].Выбрать();
	ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Ложь, Выборка);
	
	Выборка = ВыборкаПакет[4].Выбрать();
	ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Ложь, Выборка);
	
	Выборка = ВыборкаПакет[6].Выбрать();
	ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Ложь, Выборка);
	
	Выборка = ВыборкаПакет[8].Выбрать();
	ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Ложь, Выборка);
	
	Выборка = ВыборкаПакет[1].Выбрать();
	ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Истина, Выборка);
	
	Выборка = ВыборкаПакет[3].Выбрать();
	ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Истина, Выборка);
	
	Выборка = ВыборкаПакет[5].Выбрать();
	ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Истина, Выборка);
	
	Выборка = ВыборкаПакет[7].Выбрать();
	ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Истина, Выборка);
	
	Выборка = ВыборкаПакет[9].Выбрать();
	ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Истина, Выборка);
	
	Для каждого х Из тзИнтервалыСделок Цикл
		НайденаяСтрока = тзДатыОкончания.Найти(х.Проект,"Проект");
		Если НЕ НайденаяСтрока = Неопределено Тогда
		   	Если НайденаяСтрока.ОкончаниеСЗапасом > х.ДатаОкончания Тогда
				х.ДатаОкончания = НайденаяСтрока.ОкончаниеСЗапасом;
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
	
	Возврат тзИнтервалыСделок;
КонецФункции // ПолучитьИнтервалДиаграммыМассивСделок()

&НаСервере
Процедура ЗаполнитьСтрокуИнтервалСделки(тзИнтервалыСделок, Минимум, Выборка)

	
	Пока Выборка.Следующий() Цикл
		
		Проект 	= Выборка.Проект;
		текДата =  Выборка.Дата;
		
		НайденаяСтрока = тзИнтервалыСделок.Найти(Проект, "Проект");
		Если НайденаяСтрока = Неопределено Тогда
			НайденаяСтрока = тзИнтервалыСделок.Добавить();
			НайденаяСтрока.Проект = Проект;
		КонецЕсли;
		
		Если Минимум Тогда
			
			Если ЗначениеЗаполнено(НайденаяСтрока.ДатаНачала) Тогда
				НайденаяСтрока.ДатаНачала = ?(текДата > НайденаяСтрока.ДатаНачала, НайденаяСтрока.ДатаНачала, текДата);	
			Иначе
				НайденаяСтрока.ДатаНачала = текДата;	
			КонецЕсли;	
			
		Иначе
			НайденаяСтрока.ДатаОкончания = ?(текДата < НайденаяСтрока.ДатаОкончания, НайденаяСтрока.ДатаОкончания, текДата);	
		КонецЕсли;
	КонецЦикла;
	
КонецПроцедуры // ЗаполнитьСтрокуИнтервалСделки()


&НаСервере
Функция ПолучитьИнтервалДиаграммыОднаСделка(Проект,стИнтервал, тзДатыОкончания)
	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ
		|	МАКСИМУМ(ЕСТЬNULL(ДатыКомплекта.КрайПКД, ДАТАВРЕМЯ(1, 1, 1))) КАК КрайПКД,
		|	МАКСИМУМ(ЕСТЬNULL(ДатыКомплекта.КрайРКД, ДАТАВРЕМЯ(1, 1, 1))) КАК КрайРКД,
		|	МИНИМУМ(ЕСТЬNULL(ДатыКомплекта.КрайТЗ, ДАТАВРЕМЯ(1, 1, 1))) КАК КрайТЗ,
		|	МАКСИМУМ(ЕСТЬNULL(ДатыКомплекта.КрайМонтажа, ДАТАВРЕМЯ(1, 1, 1))) КАК КрайМонтажа,
		|	МАКСИМУМ(ЕСТЬNULL(ДатыКомплекта.КрайПроизводства, ДАТАВРЕМЯ(1, 1, 1))) КАК КрайПроизводства,
		|	ДатыКомплекта.НормаВремени_ПКД КАК НормаВремени_ПКД,
		|	ДатыКомплекта.НормаВремени_РКД КАК НормаВремени_РКД,
		|	ДатыКомплекта.НормаВремени_ТЗ КАК НормаВремени_ТЗ,
		|	ДатыКомплекта.НормаВремени_Монтажа КАК НормаВремени_Монтажа,
		|	ДатыКомплекта.НормаВремени_Производство КАК НормаВремени_Производство
		|ИЗ
		|	РегистрСведений.ДатыКомплекта КАК ДатыКомплекта
		|ГДЕ
		|	ДатыКомплекта.Проект = &Проект
		|
		|СГРУППИРОВАТЬ ПО
		|	ДатыКомплекта.НормаВремени_ПКД,
		|	ДатыКомплекта.НормаВремени_РКД,
		|	ДатыКомплекта.НормаВремени_ТЗ,
		|	ДатыКомплекта.НормаВремени_Монтажа,
		|	ДатыКомплекта.НормаВремени_Производство";
	
	Запрос.УстановитьПараметр("Проект",Проект);
	Выборка = Запрос.Выполнить().Выбрать();
	
	Пока Выборка.Следующий() Цикл

		стИнтервал.ДатаОкончания = ?(стИнтервал.ДатаОкончания < Выборка.КрайПКД, 			Выборка.КрайПКД, 			стИнтервал.ДатаОкончания);
		стИнтервал.ДатаОкончания = ?(стИнтервал.ДатаОкончания < Выборка.КрайРКД, 			Выборка.КрайРКД, 			стИнтервал.ДатаОкончания);
		стИнтервал.ДатаОкончания = ?(стИнтервал.ДатаОкончания < Выборка.КрайТЗ, 			Выборка.КрайТЗ, 			стИнтервал.ДатаОкончания);
		стИнтервал.ДатаОкончания = ?(стИнтервал.ДатаОкончания < Выборка.КрайМонтажа, 		Выборка.КрайМонтажа, 		стИнтервал.ДатаОкончания);
		стИнтервал.ДатаОкончания = ?(стИнтервал.ДатаОкончания < Выборка.КрайПроизводства, 	Выборка.КрайПроизводства, 	стИнтервал.ДатаОкончания);
		
		стИнтервал.ДатаНачала = РасчитатьДатаНачала(стИнтервал.ДатаНачала,Выборка.КрайПКД,			Выборка.НормаВремени_ПКД);
		стИнтервал.ДатаНачала = РасчитатьДатаНачала(стИнтервал.ДатаНачала,Выборка.КрайРКД,			Выборка.НормаВремени_РКД);
		стИнтервал.ДатаНачала = РасчитатьДатаНачала(стИнтервал.ДатаНачала,Выборка.КрайТЗ,			Выборка.НормаВремени_ТЗ);
		стИнтервал.ДатаНачала = РасчитатьДатаНачала(стИнтервал.ДатаНачала,Выборка.КрайМонтажа,		Выборка.НормаВремени_Монтажа);
		стИнтервал.ДатаНачала = РасчитатьДатаНачала(стИнтервал.ДатаНачала,Выборка.КрайПроизводства,	Выборка.НормаВремени_Производство);
		
	КонецЦикла;
	
	НайденаяСтрока = тзДатыОкончания.Найти(Проект,"Проект");
	Если НЕ НайденаяСтрока = Неопределено Тогда
		Если НайденаяСтрока.ОкончаниеСЗапасом > стИнтервал.ДатаОкончания Тогда
			стИнтервал.ДатаОкончания = НайденаяСтрока.ОкончаниеСЗапасом;
		КонецЕсли;
	КонецЕсли;
	
	Возврат стИнтервал;
КонецФункции // ПолучитьИнтервалДиаграммыМассивСделок()

&НаСервере
Функция РасчитатьДатаНачала(ДатаНачала, Край, Норма)
	Если Край = Дата(1,1,1) Тогда
		Возврат ДатаНачала; 
	КонецЕсли;
	
	Если ДатаНачала = Дата(1,1,1) Тогда
		Возврат Край; 
	КонецЕсли;
	
	ВремяДата = Край - 24 *3600 * Норма;
	Если ВремяДата > ДатаНачала Тогда
		Возврат ДатаНачала;
	КонецЕсли;
	Возврат ВремяДата;
КонецФункции // РасчитатьДатаНачала(ДатаНачала, Край, Норма)

&НаСервере
Функция ПолучитьЦвет(НаименованиеЭтапа, тзЦвета, ЭтоГруппа = Ложь, СократитьНаименование = Ложь) Экспорт
	
	Если тзЦвета = Неопределено Тогда
	
		тзЦвета = ПолучитьТаблицуЦветов();	
	
	КонецЕсли;
	
	стОтбор = Новый Структура("НаименованиеЭтапа, ЭтоГруппа", НаименованиеЭтапа, ЭтоГруппа);
	
	НайденыеСтроки = тзЦвета.НайтиСтроки(стОтбор);
	
	Если НайденыеСтроки.Количество() = 0 Тогда
		Возврат WebЦвета.ЗамшаСветлый;
	КонецЕсли;

	СократитьНаименование = НайденыеСтроки[0].СократитьНаименование;
	
	Возврат НайденыеСтроки[0].Цвет;
КонецФункции // ПолучитьЦвет()

